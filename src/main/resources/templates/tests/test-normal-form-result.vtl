#if( $option == "all" )
<div class="alert alert-warning">
  <h4>Result</h4>
  <p>$relation.name is in $result</p>
#elseif( $boolean )
  <div class="alert alert-success">
    <h4>Result</h4>
    #if( $option == "2nd" )
  	<p>$relation.name is in 2nd Normal Form</p>
  	#elseif( $option == "3rd" )
  	<p>$relation.name is in 3nd Normal Form</p>
  	#else
  	<p>$relation.name is in Boyce-Codd Normal Form</p>
  	#end
#else
  <div class="alert alert-danger">
  <h4>Result</h4>
    #if( $option == "2nd" )
  	<p>$relation.name is not in 2nd Normal Form</p>
  	#elseif( $option == "3rd" )
  	<p>$relation.name is not in 3nd Normal Form</p>
  	#else
  	<p>$relation.name is not in Boyce-Codd Normal Form</p>
  	#end
#end
</div>