#if ( !$attributes.isEmpty() )
  <h4>Attributes</h4>
  <ul class="list-group">
  #foreach( $attr in $attributes )
    <li class="list-group-item">
      <label class="checkbox-inline">
        <input name="__attr__$attr" type="checkbox" value="$attr">$attr
      </label>
    </li>
  #end
  </ul>
#end