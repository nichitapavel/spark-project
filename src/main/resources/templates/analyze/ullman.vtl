<div class="col-sm-10 blog-main kbd">
  <div class="blog-post">
    <h1>Calculate Ullman!</h1>
#parse( $result )
    <form action="/ullman" method="POST" >
	  <p class="alert alert-success">Attributos: </p>
	  <ul class="list-group">
	  #foreach( $attr in $attributes )
	    <li class="list-group-item"><label class="checkbox-inline"><input name="attr-$attr" type="checkbox" value="$attr">$attr</label></li>
	  #end
	  </ul>
	  
	  <p class="alert alert-success">Relations: </p>
	    <ul class="list-group">
	    #foreach( $relation in $relations )
	      <li class="list-group-item"><label class="radio-inline"><input name="rel-$relation.name" type="radio" value="$relation.name">$relation</label></li>
	    #end
	  </ul>
	  <button type="submit" class="btn">Go!</button>
	</form>
  </div>
</div>